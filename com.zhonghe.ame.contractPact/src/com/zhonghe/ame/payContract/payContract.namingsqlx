<?xml version="1.0" encoding="UTF-8"?>
<sqlMap>
    
    <resultMap class="commonj.sdo.DataObject" id="purSupplier">
        <result column="CUSTID" javaType="int" property="custid"/>
        <result column="CUSTNAME" javaType="string" property="custname"/>
    </resultMap>    

    <select id="queryPurSupplierIsqualified" resultMap="purSupplier">
    	SELECT CUSTID,CUSTNAME FROM PUR_SUPPLIER WHERE (CUSTTYPE IN(0,1,2) AND isqualified IN (1,2)) OR CUSTTYPE IN (3,4)
    </select>    

</sqlMap>
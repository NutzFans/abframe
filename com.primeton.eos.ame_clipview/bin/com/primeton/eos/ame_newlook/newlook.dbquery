<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<QueryEntityList>
    <QueryEntity name="com.primeton.eos.ame_newlook.newlook.GetMyFastMenu">
        <config db="default" type="sql"><![CDATA[SELECT t.OPERATORID,t.ORDERNO,a.MENUID as ids,a.MENULABEL as text,case when a.IMAGEPATH is null then ' clip-file-2' else a.IMAGEPATH end as icon,'/default'+a.MENUACTION as url FROM AC_OPERSHORTCUT t
 LEFT OUTER JOIN AC_MENU a ON t.FUNCCODE=a.FUNCCODE]]></config>
    </QueryEntity>
</QueryEntityList>

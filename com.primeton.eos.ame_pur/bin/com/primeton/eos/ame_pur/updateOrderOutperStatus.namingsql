<sqlMap namespace="com.primeton.eos.ame_pur.updateOrderOutperStatus">
    <update id="updatestatus"><![CDATA[
		    update PUR_ORDER_OUTPER 
		    set 
		    	currentstatus = '0' 
		    where 
		    	OUTPERNO = #outperno# 
		]]></update>
</sqlMap>